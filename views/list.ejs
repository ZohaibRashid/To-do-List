 <%- include("header") -%>
  <div class = "box" id = "heading" >
<h1 ><%= listType %></h1>

</div>

<div class = "box">
<% for(var i =0; i<item.length; i++) { %>

   <div class = "item">
  <input type="checkbox">
  <p><%= item[i] %></p>
</div>


<%} %>


<form action="/" class = "item" method = "POST">
  <input type="text" name = "newItem" autocomplete="off" placeholder = "New Item">
  <button type = "submit" name = "list" value = <%=listType %>>+</button>
  </form>
</div>


 
</body>
</html>


<%- include("footer") -%>















